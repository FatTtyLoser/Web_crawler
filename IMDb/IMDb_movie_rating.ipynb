   "source": [
    "import requests\n",
    "import urllib.request\n",
    "import pandas as pd\n",
    "import os\n",
    "import time\n",
    "import random\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "with open('檔案路徑\\\\movie_list.txt', mode='r', encoding = 'utf8') as f:\n",
    "    a = f.read()\n",
    "    movie_list = a.split('\\n')\n",
    "    while '' in movie_list:\n",
    "        movie_list.remove('')\n",
    "n=0\n",
    "list = []\n",
    "movie_page_list = []\n",
    "\n",
    "UserAgent = 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/68.0.3440.84 Safari/537.36'\n",
    "headers = {\n",
    "    'user-agent':UserAgent\n",
    "}\n",
    "\n",
    "for M in range(len(movie_list)):\n",
    "    try:\n",
    "        movie_name = movie_list[n]\n",
    "        url ='https://www.imdb.com/find?q={}'\n",
    "        res = requests.get(url=url.format(movie_name), headers=headers)\n",
    "        html = res.text\n",
    "        soup = BeautifulSoup(html, 'html.parser')\n",
    "        movie_html = soup.select_one('td[class=\"result_text\"]')\n",
    "        movie_page = movie_html.select('a')[0]['href']\n",
    "        movie_page_list.append('https://www.imdb.com' + movie_page)\n",
    "        time.sleep(random.uniform(1,3))\n",
    "        try:\n",
    "            url ='{}'\n",
    "            res = requests.get(url=url.format(movie_page_list[n]), headers=headers)\n",
    "            html = res.text\n",
    "            soup = BeautifulSoup(html, 'html.parser')\n",
    "            movie_rating = soup.select_one('span[class=\"AggregateRatingButton__RatingScore-sc-1ll29m0-1 iTLWoV\"]').text+'/10'\n",
    "            rating_total = soup.select_one('div[class=\"AggregateRatingButton__TotalRatingAmount-sc-1ll29m0-3 jkCVKJ\"]').text\n",
    "        except:\n",
    "            print('第' + str(n+1) + '部電影IMDb尚未收錄')\n",
    "        else:\n",
    "            list.append(movie_name)\n",
    "            list.append(movie_rating)\n",
    "            list.append(rating_total)\n",
    "        finally:\n",
    "            print('第' + str(n+1) + '次迴圈結束')\n",
    "    except:\n",
    "        print('第' + str(n+1) + '次爬蟲出錯，請檢查程式碼')\n",
    "    n = n+1\n",
    "    time.sleep(random.uniform(1,3))\n",
    "step = 3\n",
    "movie_colum = [list[i:i + step] for i in range(0, len(list), step)]\n",
    "print(movie_colum)\n",
    "movie_df = pd.DataFrame(movie_colum)\n",
    "movie_df.index = movie_df.index + 1  # 自訂索引值\n",
    "movie_df.columns = [\"Movie_name\",\"Rating\", \"Rating_total\"]\n",
    "movie_df.to_csv('目標路徑/IMDb_movie_rating.csv',encoding=\"utf_8_sig\")"
   ]
