    "UserAgent = 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/68.0.3440.84 Safari/537.36'\n",
    "headers = {\n",
    "    'user-agent':UserAgent\n",
    "}\n",
    "movie_list = []\n",
    "res = requests.get(url=url, headers=headers)\n",
    "html = res.text\n",
    "soup = BeautifulSoup(html, 'html.parser')\n",
    "titlelist = soup.select('span[lang=\"en\"]')\n",
    "for M in titlelist:\n",
    "    movie_list.append(M.text + '\\n')\n",
    "path = '儲存路徑\\\\movie_list.txt'\n",
    "f = open(path,'w',encoding=\"utf-8\")\n",
    "f.writelines(movie_list)\n",
    "f.close()"
